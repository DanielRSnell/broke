			"fontSize": "var(--wp--preset--font-size--base)",
			"lineHeight": "1.6",
			"fontWeight": "400"
		},
		"spacing": {
			"blockGap": "6rem",
			"padding": {
				"top": "0",
				"right": "0",
				"bottom": "0",
				"left": "0"
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--neutral-300)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--white)"
					}
				}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--white)",
					"text": "var(--wp--preset--color--neutral-950)"
				},
				"border": {
					"radius": "0.5rem"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)",
					"fontWeight": "500"
				},
				":hover": {
